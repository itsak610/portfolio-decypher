    .loader {
        position: absolute;
        z-index: 999999;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        background: white;
        display: flex;
        justify-content: center;
        align-items: center;
        background: #080808;
    }


    .loader.hidden1 {
        transition: all 1s ease-in-out;
        animation: fadeOut 0.5s;
        animation-fill-mode: forwards;
    }
    @keyframes fadeIn {
        0%{
            opacity: 0;
        }
        50% {
            opacity: 0.9;
        }
        100% {
            opacity: 1;
        }
    }
    @keyframes fadeOut {
        from {
            opacity: 1;
        }
        to {
            opacity: 0;
        }
    }

    .loader {
        max-width: 100vw;
        background: #0f0c29;
        width: 100%;
        height: 100%;
        position: fixed;
        display: flex;
    }

    
    .loadcontainer {
        width: 100%;
        height: 100%;
        color: #222;
        overflow: hidden;
    }

    .side {
        position: relative;
        display: flex;
        justify-content: center;
        align-items: center;
        top: 50%;
        transform: translate(0, -50%);
    }
    .side > * {
        opacity:0.6;
        animation: bottom 1.5s ease infinite;
        position: relative;
        max-width: 100%;
        max-height: 100%;
        width: 20%;
        height: 20%;
        margin:20px;
    }
    .two{
        animation-delay:0.2s
    }
    .three{
        animation-delay:0.4s
    }

    @keyframes bottom2 {
        0% {
            transform: rotateY(0deg);
        }
        25% {
            transform: rotateY(0deg);
        }
        50% {
            transform: rotateY(180deg);
        }
        75% {
            transform: rotateY(360deg);
        }
        100% {
            transform: rotateY(360deg);
        }
    }
    @keyframes bottom {
        0% {
            opacity:0.4;
        }
        80%{
            opacity:1;
        }
        100% {
            opacity:0.4;
        }
    }
    .outro{
        animation: exit 0.3s;
        animation-delay : 0.5s;
        animation-fill-mode: forwards;
    }

    @keyframes exit {
        from {
            margin-top: 0vh;
            display: block;
        }
        to {
            margin-top: 100vh;
            display: none;
        }
    }
    @media (max-width:750px) {
        .side > * {
            height: 20%;
            width: 20%;
        }   
    }
    @media (min-width:750px) and (max-width:1112px) {
        .side > * {
            height: 20%;
            width: 20%;
        }   
    }
    .question-card{
        word-wrap: break-word;
    }
    .hintbox{position:fixed;right:0;margin-top:-50vh;margin-right:50px;height:30vh;width:20vw;background:#030711;display:flex;justify-content:center;align-items:center;flex-flow:column}
    .hintbox h1{text-align:center !important;padding-top:30px}
    .hintbox ul, .hintbox li{padding:0;margin:0;word-wrap: break-word;text-align:center;list-style-type:none}
    @media (max-width:1300px){
        .hintbox{
            display:none;
        }
    }
