    h2 The event will start on 7th of August at 00:00:00.
  script.
    var path = window.location.pathname;
    var page = path.split("/").pop();
    if(page==='leaderboard'){
        document.getElementById("leaderboardnav").classList.toggle("active");
        document.getElementById("leaderboardnav").style.backgroundSize = "80px 2px";
    }
    else if(page==='play'){
        document.getElementById("playnav").classList.toggle("active");
        document.getElementById("playnav").style.backgroundSize = "30px 2px";
    }
    window.addEventListener("DOMContentLoaded", function(event) {
      if(document.getElementById('answer')) {
        document.getElementById('answer').addEventListener('input', function(){
          this.value = this.value.replace(/\s+/g, '').toLowerCase();
        });
      }
    });
    
